<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/a81"
    tools:context=".questList$PlaceholderFragment">

    <TextView
        android:id="@+id/section_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/activity_vertical_margin"
        android:layout_marginEnd="@dimen/activity_horizontal_margin"
        android:layout_marginStart="@dimen/activity_horizontal_margin"
        android:layout_marginTop="@dimen/activity_vertical_margin" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="168dp"
        android:layout_height="243dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/section_label"
        android:layout_marginTop="43dp"
        android:src="@drawable/sukablyat" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="168dp"
        android:layout_height="243dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/imageView13"
        android:layout_marginEnd="13dp"
        android:src="@drawable/realnolubazaebal" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="168dp"
        android:layout_height="243dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/section_label"
        android:layout_marginBottom="27dp"
        android:src="@drawable/pidor" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="168dp"
        android:layout_height="243dp"
        android:layout_alignEnd="@+id/imageView3"
        android:layout_alignTop="@+id/imageView6"
        android:src="@drawable/lubazaebal" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="205dp"
        android:layout_height="75dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="194dp"
        android:src="@drawable/backhorizont" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="52dp"
        android:layout_marginTop="203dp"
        android:text="Відшукай"
        android:textColor="@android:color/background_dark"
        android:textSize="20sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="99dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView5"
        android:layout_below="@+id/textView5"
        android:layout_marginStart="7dp"
        android:text="свободу"
        android:textColor="@android:color/background_dark"
        android:textSize="20sp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="118dp"
        android:layout_height="84dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="34dp"
        android:layout_marginEnd="36dp"
        android:src="@drawable/backhorizont" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/imageView14"
        android:layout_marginEnd="71dp"
        android:text="Здай"
        android:textColor="@android:color/background_dark"
        android:textSize="20sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView13"
        android:layout_below="@+id/textView13"
        android:layout_marginTop="-84dp" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView13"
        android:layout_below="@+id/textView13"
        android:layout_marginStart="-10dp"
        android:text="9 лабу"
        android:textColor="@android:color/background_dark"
        android:textSize="20sp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="121dp"
        android:layout_height="80dp"
        android:layout_alignEnd="@+id/textView6"
        android:layout_alignTop="@+id/imageView14"
        android:src="@drawable/backhorizont" />

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="118dp"
        android:layout_height="103dp"
        android:layout_alignBottom="@+id/imageView13"
        android:layout_alignStart="@+id/imageView14"
        android:src="@drawable/backhorizont" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView5"
        android:layout_alignTop="@+id/imageView14"
        android:text="Кинь собі"
        android:textColor="@android:color/background_dark"
        android:textSize="20sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView6"
        android:layout_below="@+id/textView13"
        android:text="виклик"
        android:textColor="@android:color/background_dark"
        android:textSize="20sp"
        android:textStyle="bold" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView13"
        android:layout_alignTop="@+id/textView5"
        android:text="Зроби"
        android:textColor="@android:color/background_dark"
        android:textSize="20sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/imageView14"
        android:layout_below="@+id/textView5"
        android:text="неможливе"
        android:textColor="@android:color/background_dark"
        android:textSize="20sp"

        android:textStyle="bold" />

    <Button
        android:id="@+id/freedom"
        android:layout_width="158dp"
        android:layout_height="250dp"
        android:layout_alignStart="@+id/section_label"
        android:layout_alignTop="@+id/imageView13"
        android:background="@android:color/transparent"

        />

    <Button
        android:id="@+id/impossible"
        android:layout_width="158dp"
        android:layout_height="250dp"
        android:layout_alignStart="@+id/imageView3"

        android:layout_alignTop="@+id/imageView13"
        android:background="@android:color/transparent" />

    <Button
        android:id="@+id/call"
        android:layout_width="158dp"
        android:layout_height="250dp"
        android:layout_alignStart="@+id/section_label"

        android:layout_alignTop="@+id/imageView6"
        android:background="@android:color/transparent" />

    <Button
        android:id="@+id/nine"
        android:layout_width="158dp"
        android:layout_height="250dp"
        android:layout_alignStart="@+id/imageView3"

        android:layout_alignTop="@+id/imageView6"
        android:background="@android:color/transparent" />

</RelativeLayout>